    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.8",
    "@types/pug": "^2.0.6",
    "@types/validator": "^13.7.15",
    "ndb": "^1.1.5",
    "nodemon": "^2.0.22",
    "parcel-bundler": "^1.12.5",
